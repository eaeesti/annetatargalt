{
  "kind": "collectionType",
  "collectionName": "donors",
  "info": {
    "singularName": "donor",
    "pluralName": "donors",
    "displayName": "Donor",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
